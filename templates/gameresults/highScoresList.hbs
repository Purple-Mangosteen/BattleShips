<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Score</th>
            <th>Maps</th>
        </tr>
    </thead>
    <tbody>

        {{#each results}}
        <tr>
            <td>{{username}}</td>
            <td>{{totalScore}}</td>
            <td>{{gamesPlayed}}</td>
        </tr>

        {{/each}}
    </tbody>
</table>